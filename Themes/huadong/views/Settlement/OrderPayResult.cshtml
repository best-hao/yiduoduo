@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>支付提醒</title>
    
    <meta name="format-detection" content="telephone=no">
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="default" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <link href="@{@WorkContext.CSSCDN}/mobile/content/css/weui.css?version=16011201" rel="stylesheet" media="screen" />
</head>
<body>
    <div class="container" id="container">
        <div class="msg">
            <div class="weui_msg">
                <div class="weui_icon_area"><i class="weui_icon_success weui_icon_msg"></i></div>
                <div class="weui_text_area">
                    <h2 class="weui_msg_title">支付成功</h2>
                    <p class="weui_msg_desc"></p>
                </div>
                <div class="weui_opr_area">
                    <p class="weui_btn_area">
                        <a href="/m/order/list" class="weui_btn weui_btn_primary">查看订单</a>
                    </p>
                </div>
            </div>
        </div>
    </div>
</body>
</html>